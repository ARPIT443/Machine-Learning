import matplotlib.pyplot as plt
   # importing visualization library
   
x=[2,3,5,6]  # data to be plot on a graph
x1=[4,3,8]
y1=[2,9,7]
y=[9,4,7,8]

plt.xlabel("X_axies")
plt.ylabel("Y_axies")  #labeling on x and y axes 

plt.plot(x,y,label="first plot")   #   this will draw a straight  line 
plt.plot(x1,y1,label="second plot")   #   this will draw a straight  line

plt.bar(x,y)   #   to plot bar  graphs 
plt.bar(x1,y1)   #   to plot bar  graphs 

plt.grid(color='green')  #  to form  grid  in graph 
plt.legend() #   to show labels with plot

plt.xlim(0,12)  #  to show  min and max  number in x  axis 
plt.ylim(0,15)   #  setting y axis limit
plt.show()
